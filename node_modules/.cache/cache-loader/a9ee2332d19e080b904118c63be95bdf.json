React from 'react';\n  import { mdx } from '@mdx-js/react';\n\n  export const frontMatter = {\n\tid: 'zh_intro',\n\ttitle: '项目简介',\n\tslug: '../zh/intro'\n};\nexport const metadata = {\n  \"unversionedId\": \"zh_intro\",\n  \"id\": \"zh_intro\",\n  \"isDocsHomePage\": false,\n  \"title\": \"项目简介\",\n  \"description\": \"export const Highlight = ({children, color}) => ( <span style={{\",\n  \"source\": \"@site/docs/zh_1_intro.md\",\n  \"slug\": \"/zh/intro\",\n  \"permalink\": \"/docs/zh/intro\",\n  \"editUrl\": \"https://github.com/facebook/docusaurus/edit/master/website/docs/zh_1_intro.md\",\n  \"version\": \"current\",\n  \"sidebar\": \"someSidebar\",\n  \"previous\": {\n    \"title\": \"Guide\",\n    \"permalink\": \"/docs/en/guide\"\n  },\n  \"next\": {\n    \"title\": \"代币经济\",\n    \"permalink\": \"/docs/zh/token\"\n  }\n};\n  /* @jsx mdx */\n\nexport const rightToc = [{\n  value: '简介',\n  id: '简介',\n  children: []\n}, {\n  value: '关于产品',\n  id: '关于产品',\n  children: [{\n    value: '交易',\n    id: '交易',\n    children: []\n  }, {\n    value: '流动性池子',\n    id: '流动性池子',\n    children: []\n  }, {\n    value: '持币生息',\n    id: '持币生息',\n    children: []\n  }, {\n    value: '投票',\n    id: '投票',\n    children: []\n  }, {\n    value: '乐透',\n    id: '乐透',\n    children: []\n  }]\n}, {\n  value: '资讯',\n  id: '资讯',\n  children: []\n}];\nexport const Highlight = ({\n  children,\n  color\n}) => <span style={{\n  backgroundColor: color,\n  borderRadius: '3px',\n  color: '#fff',\n  padding: '0.1rem'\n}}>{children}</span>;\n\nconst layoutProps = {\n  rightToc,\nHighlight\n};\nconst MDXLayout = \"wrapper\"\nexport default function MDXContent({\n  components,\n  ...props\n}) {\n  return <MDXLayout {...layoutProps} {...props} components={components} mdxType=\"MDXLayout\">\n\n\n    <h2 {...{\n      \"id\": \"简介\"\n    }}>{`简介`}</h2>\n    <p><img alt={\"img\"} src={require(\"!url-loader?limit=10000&name=assets/images/[name]-[hash].[ext]&fallback=file-loader!./../static/img/banner.png\").default} /></p>\n    <p>{`松饼交换是一个自动做市商(AMM)自动做市商算法的上链流动性交换协议。藉由智能合约来达到自动执行交易、且自动提供流动性的目的。用户可以在这边享受到最棒的价格、以及最棒的深度。PancakeSwap 目前是透过链上(on-chain)的价格预言机(BAND Protocol)的价格报价来获取某项资产在当前市场的准确价格。`}</p>\n    <p>{`CAKE 以及 SYRUP 是我们的治理代币，未来或执行回购销毁模型、来保持代币通缩。`}</p>\n    <p>{`CAKE 以及 SYRUP 将完全通过流动性挖矿进行分配，没有预售、预挖和团队预留份额。`}</p>\n    <p>{`PancakeSwap 收取 0.2%的交易费用（0.17%将分给流动性提供者、0.03%提供给 PancakeSwap Treasury）。`}</p>\n    <p>{`当你在`}<a parentName=\"p\" {...{\n        \"href\": \"https://pancakeswap.finance/farms\"\n      }}>{`Farm`}</a>{`页面抵押 PancakeSwap 的流动性提供(LP)代币时，会获得对应比例的 CAKE 奖励。`}</p>\n    <h4 {...{\n      \"id\": \"你的理想生活是什么\"\n    }}><Highlight color=\"#25c2a0\" mdxType=\"Highlight\">{`你的理想生活是什么?`}</Highlight></h4>\n    <p>{`是成堆的煎饼，糖浆，奶油吗？`}</p>\n    <p>{`那就在结束早餐后，来 ancakeSwap 挖更多的松饼吧！`}</p>\n    <p>{`松饼协议(PancakeSwap) 目标是成为币安智能链上排名第一的流动性提供者（并且我们已经达到了），利用新的，创新的游戏化耕作机制，让链上的去中心化金融运作得更美好！快来加速我们吧！`}</p>\n    <p><strong parentName=\"p\">{`为何选择币安智能链(BSC)?`}</strong></p>\n    <p>{`我想道理很简单！简单来说，为什么我们要开一辆运行成本更高、但是速度较慢的汽车？`}</p>\n    <p>{`虽然币安智能链目前可能还非常年轻，但我们相信币安的能力，并在可预见的未来将其紧密逼近。当然，在以太坊上发布将是最容易的事情，并且可以轻易地获得更快的早期使用率，但是我们喜欢接受挑战！并且押注币安智能链的未来！`}</p>\n    <h4 {...{\n      \"id\": \"快速、便宜、轻松交易！\"\n    }}>{`快速、便宜、轻松交易！`}</h4>\n    <hr></hr>\n    <p><strong parentName=\"p\">{`哪些特点让松饼协议如此迷人呢？`}</strong></p>\n    <p>{`我们为了提供最优质的交易体验。`}</p>\n    <p>{`这意味着什么？我们勇于创新、且带来新事物，并且专注在产品打磨。`}</p>\n    <hr></hr>\n    <h2 {...{\n      \"id\": \"关于产品\"\n    }}>{`关于产品`}</h2>\n    <h3 {...{\n      \"id\": \"交易\"\n    }}>{`交易`}</h3>\n    <p>{`对于松饼协议来说 `}<a parentName=\"p\" {...{\n        \"href\": \"https://exchange.pancakeswap.finance/#/swap\"\n      }}><strong parentName=\"a\">{`代币互换`}</strong></a>{` 是最目前币安智能链上`}<strong parentName=\"p\">{`深度最好、交易量最大`}</strong>{`的流动性提供池。`}</p>\n    <p>{`提供给交易所的流动性来自将其代币投入\"Pools”的流动性提供者\"。作为交换，他们获得 FLIP（PancakeSwap 流动性令牌)，也可以将其放样以在“农场”中赚取 CAKE 令牌。`}</p>\n    <p>{`而当您在交易所进行代币交换时，您将支付`}<strong parentName=\"p\">{`0.2％的交易费用`}</strong>{` ，细分如下：`}</p>\n    <p><strong parentName=\"p\">{`0.17%`}</strong>{` - 回馈至流动性池子里面、奖励流动性提供者。`}</p>\n    <p><strong parentName=\"p\">{`0.03%`}</strong>{` - 锁定至煎饼基金里面。`}</p>\n    <h3 {...{\n      \"id\": \"流动性池子\"\n    }}>{`流动性池子`}</h3>\n    <h4 {...{\n      \"id\": \"挖矿\"\n    }}>{`挖矿`}</h4>\n    <p><a parentName=\"p\" {...{\n        \"href\": \"https://exchange.pancakeswap.finance/#/pool\"\n      }}><strong parentName=\"a\">{`煎饼协议`}</strong>{` `}</a>{`允许您通过将代币添加到流动资金池或“ LP”来提供流动资金。`}</p>\n    <p>{`当您将代币添加到流动资金池(LP)中时，您将收到 FLIP 代币（PancakeSwap 的流动性提供商代币版本）。`}</p>\n    <p>{`例如，如果您将$CAKE和$BNB 存入流动资金池，您将收到 `}<strong parentName=\"p\">{`CAKE-BNB`}</strong>{` FLIP 代币。`}</p>\n    <p>{`您收到的 FLIP 代币数量代表您在 CAKE-BNB 流动资金池中的份额。您也可以随时通过消除流动性来赎回资金。`}</p>\n    <hr></hr>\n    <h4 {...{\n      \"id\": \"关于提供流动性的风险\"\n    }}>{`关于提供流动性的风险`}</h4>\n    <p>{`提供流动性并非没有风险，因为您可能会遭受无常损失。`}</p>\n    <blockquote>\n      <p parentName=\"blockquote\"><a parentName=\"p\" {...{\n          \"href\": \"https://blog.bancor.network/beginners-guide-to-getting-rekt-by-impermanent-loss-7c9510cb2f22\"\n        }}>{`Nate Hindman`}</a>{`:`}</p>\n      <p parentName=\"blockquote\">{`\"impermanent loss is the difference between holding tokens in an AMM and holding them in your wallet.”`}</p>\n    </blockquote>\n    <p>{`对于流动性提供者来说，这并不全是坏事，因为您还将获得交易费形式的奖励。每当有人交易 PancakeSwap 时，交易者将支付 0.2％的费用，其中 0.17％被添加到他们交易的掉期对的流动性池中。`}</p>\n    <p>{`例如:`}</p>\n    <ul>\n      <li parentName=\"ul\">{`假设有 10 个 FLIP 代币 等值代表 10 枚 CAKE、以及 10 枚 BNB.`}</li>\n      <li parentName=\"ul\">{`1 FLIP 代币 = 1 CAKE + 1 BNB`}</li>\n      <li parentName=\"ul\">{`某用户选择 10 CAKE 换取 10 BNB.`}</li>\n      <li parentName=\"ul\">{`另外个用户选择 10 BNB 换取 10 CAKE.`}</li>\n      <li parentName=\"ul\">{`那么 CAKE/BNB 流动性池将会成为 10.017 CAKE & 10.017 BNB.`}</li>\n      <li parentName=\"ul\">{`则 FLIP 代币当下等值为 1.00017 CAKE + 1.00017 BNB.`}</li>\n    </ul>\n    <h4 {...{\n      \"id\": \"如何最大化你的收益？\"\n    }}>{`如何最大化你的收益？`}</h4>\n    <p>{`为了让你手中的代币可以最大化其价值，我们建议你将提供动性后的 FLIP 代币存入 `}<a parentName=\"p\" {...{\n        \"href\": \"https://pancakeswap.finance/\"\n      }}>{`CAKE farms`}</a>{` 换取更多收益。（此时你依旧能获取提供流动性后的 0.17%手续费奖励)`}</p>\n    <h3 {...{\n      \"id\": \"持币生息\"\n    }}>{`持币生息`}</h3>\n    <p>{`Farming`}</p>\n    <h3 {...{\n      \"id\": \"投票\"\n    }}>{`投票`}</h3>\n    <p>{`SYRUP: 治理型代币。`}</p>\n    <h3 {...{\n      \"id\": \"乐透\"\n    }}>{`乐透`}</h3>\n    <p>{`未来将为 CAKE 提供更多赋能。`}</p>\n    <h2 {...{\n      \"id\": \"资讯\"\n    }}>{`资讯`}</h2>\n    <p><a parentName=\"p\" {...{\n        \"href\": \"https://pancakeswap.info/\"\n      }}>{`https://pancakeswap.info/`}</a></p>\n    </MDXLayout>;\n}\n\n;\nMDXContent.isMDXComponent = true;\n  "]}]}